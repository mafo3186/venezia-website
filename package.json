    "sanity-plugin-asset-source-unsplash": "^3.0.1",
    "sanity-plugin-media": "^2.3.2",
    "server-only": "^0.0.1",
    "three": "^0.168.0",
    "three-pathfinding": "^1.3.0",
    "typescript": "5.6.2"
